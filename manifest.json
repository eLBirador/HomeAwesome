{

    "manifest_version": 2,
    "name": "Home Awesome",
    "version": "0.0.3",

    "description": "Customize New Tabs",
    "short_name": "Home Awesome",


    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "offline_enabled": true,
    "icons": {
        "16": "icons/icon16.png",
        "64": "icons/icon64.png",
        "128": "icons/icon128.png"
    },

    "applications": {
        "gecko": {
            "strict_min_version": "54.0"
        }
    }

}